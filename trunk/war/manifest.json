{
  "name": "Bookmark service",
  "description": "This extension adds a Bookmark service for http://mismarks.appspot.com",
  "version": "1.0",
  "permissions": [
    "tabs", 
    "http://mismarks.appspot.com/"
  ],
  "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Mismarks bookmarks",
      "popup": "Extension.html"
  }
}